dmgTpDemi

type		dmgMagical
function	/ &VP_@entityStats_VP_stat_statHp amount
dstat		statHp

dmgTpMagicMissle

type		dmgMagical
function	min 0 - amount / &VP_@entityStats_VP_stat_statIntellect 10 
# function	amount
dstat		statHp


dmgTpHeal

type		dmgMagical
function	* + / &VP_@entityStats_VP_stat_statIntellect 10 amount gate random 0 20 10
dstat		statHp

dmgTpPunch

type		dmgPhysical
function	min 0 - amount / &VP_@entityStats_VP_stat_statHardiness 10 
# function	amount
dstat		statHp

dmgTpDeath

type		dmgMagical
function	* &VP_@entityStats_VP_stat_statHp gate random 0 10 5
dstat		statHp
